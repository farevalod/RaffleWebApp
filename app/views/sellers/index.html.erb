<p id="notice"><%= notice %></p>

<h1>Sellers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Rut</th>
      <th>Email</th>
      <th>Phone number</th>
      <th>Num in institution</th>
      <th>Num of logins</th>
      <th>Email confirmed</th>
      <th>Confirm token</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.name %></td>
        <td><%= seller.rut %></td>
        <td><%= seller.email %></td>
        <td><%= seller.phone_number %></td>
        <td><%= seller.num_in_institution %></td>
        <td><%= seller.num_of_logins %></td>
        <td><%= seller.email_confirmed %></td>
        <td><%= seller.confirm_token %></td>
        <td><%= link_to 'Show', seller %></td>
        <td><%= link_to 'Edit', edit_seller_path(seller) %></td>
        <td><%= link_to 'Destroy', seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller', new_seller_path %>
