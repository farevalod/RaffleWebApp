<table>
  <thead>
  <tr>
    <th>Num</th>
    <th>Nombre Vendedor</th>
    <th>Boletos Vendidos</th>
    <th>Boletos Pagados</th>
    <th>Talonario Pagado Completo</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @books.each do |book| %>
    <tr>
      <td><%= book.num_in_institution %></td>
      <td><strong><%= book.seller.name %></strong></td>
      <td><%= book.tickets_sold %></td>
      <td><%= book.tickets_paid %></td>
      <td><%= book.paid_at %></td>
      <td><%= button_to 'Mostrar', book_path(book.id) , method: :get %></td>
    </tr>
  <% end %>
  </tbody>
</table>